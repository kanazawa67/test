<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
"http://struts.apache.org/dtds/struts-2.3.dtd">
<!-- Strutsの詳細設定 -->
<struts>
	<constant name="struts.devMode" value="true"/>
	<package name="com.internousdev.ecsite.action" extends="struts-default">

	<!--GoHomeAction -->
	<action name="GoHomeAction"
	class="com.internousdev.ecsite.action.GoHomeAction" method="execute">
		<result name="success">home.jsp</result>
	</action>

	<!--HomeAction -->
	<action name="HomeAction" class="com.internousdev.ecsite.action.HomeAction"
	method="execute">
	<result name="login">login.jsp</result>
	<result name="success">buyItem.jsp</result>
	</action>

	<!--LoginAction -->
	<action name="LoginAction" class="com.internousdev.ecsite.action.LoginAction"
	method="execute">
		<result name="success">buyItem.jsp</result>
		<result name="login">kanrishagamen.jsp</result>
		<result name="error">login.jsp</result>
	</action>

	<!--UserCreateAction -->
	<action name="UserCreateAction"
	class="com.internousdev.ecsite.action.UserCreateAction" method="execute">
	<result name="success">userCreate.jsp</result>
	<result name="back">login.jsp</result>
	</action>

	<!--UserCreateConfirmAction -->
	<action name="UserCreateConfirmAction"
	class="com.internousdev.ecsite.action.UserCreateConfirmAction" method="execute">
	<result name="success">userCreateConfirm.jsp</result>
	<result name="error">userCreate.jsp</result>
	</action>

	<!--UserCreateCompleteAction -->
	<action name="UserCreateCompleteAction"
	class="com.internousdev.ecsite.action.UserCreateCompleteAction" method="execute">
	<result name="success">userCreateComplete.jsp</result>
	</action>

	<!--BuyItemAction -->
	<action name="BuyItemAction"
	class="com.internousdev.ecsite.action.BuyItemAction" method="execute">
	<result name="success">buyItemConfirm.jsp</result>
	<result name="error">buyItem.jsp</result>
	</action>

	<!--BuyItemConfirmAction -->
 	<action name="BuyItemConfirmAction"
 	class="com.internousdev.ecsite.action.BuyItemConfirmAction" method="execute">
 	<result name="success">buyItemComplete.jsp</result>
	<result name="back">buyItem.jsp</result>
 	</action>


	<!--MyPageAction -->
	<action name="MyPageAction" class="com.internousdev.ecsite.action.MyPageAction"
	method="execute">
	<result name="success">myPage.jsp</result>
	</action>

	<!--LogoutAction -->
	<action name="LogoutAction" class="com.internousdev.ecsite.action.LogoutAction"
	method="execute">
	<result name="success">home.jsp</result>
	</action>

	<!--ShohinIchiranAction -->
	<action name="ShohinIchiranAction" class="com.internousdev.ecsite.action.ShohinIchiranAction"
	method="execute">
	<result name="success">shohinIchiran.jsp</result>
	</action>

	<!--GoKanriAction -->
	<action name="GoKanriAction"
	class="com.internousdev.ecsite.action.GoKanriAction" method="execute">
		<result name="success">kanrishagamen.jsp</result>
	</action>


	<!--ShohinCreateAction -->
	<action name="ShohinCreateAction"
	class="com.internousdev.ecsite.action.ShohinCreateAction" method="execute">
	<result name="success">shohinCreate.jsp</result>
	</action>

	<!--ShohinCreateConfirmAction -->
	<action name="ShohinCreateConfirmAction"
	class="com.internousdev.ecsite.action.ShohinCreateConfirmAction" method="execute">
	<result name="success">shohinCreateConfirm.jsp</result>
	<result name="error">shohinCreate.jsp</result>
	</action>

	<!--ShohinCreateCompleteAction -->
	<action name="ShohinCreateCompleteAction"
	class="com.internousdev.ecsite.action.ShohinCreateCompleteAction" method="execute">
	<result name="success">shohinCreateComplete.jsp</result>
	</action>

	<!--UserIchiranAction -->
	<action name="UserIchiranAction" class="com.internousdev.ecsite.action.UserIchiranAction"
	method="execute">
	<result name="success">userIchiran.jsp</result>
	</action>

	<!--UserCreateAction2 -->
	<action name="UserCreateAction2"
	class="com.internousdev.ecsite.action.UserCreateAction2" method="execute">
	<result name="success">userCreate2.jsp</result>
	<result name="back">login.jsp</result>
	</action>

	<!--UserCreateConfirmAction2 -->
	<action name="UserCreateConfirmAction2"
	class="com.internousdev.ecsite.action.UserCreateConfirmAction2" method="execute">
	<result name="success">userCreateConfirm2.jsp</result>
	<result name="error">userCreate2.jsp</result>
	</action>

	<!--UserCreateCompleteAction2 -->
	<action name="UserCreateCompleteAction2"
	class="com.internousdev.ecsite.action.UserCreateCompleteAction2" method="execute">
	<result name="success">userCreateComplete2.jsp</result>
	</action>









</package>
</struts>